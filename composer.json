{
    "name": "laravolt/auth",
    "description": "Laravel auth extended",
    "keywords": [
        "laravolt",
        "auth",
        "laravel"
    ],
    "homepage": "https://github.com/laravolt/auth",
    "license": "MIT",
    "authors": [
        {
            "name": "Bayu Hendra Winata",
            "email": "id.laravel@gmail.com",
            "homepage": "http://id-laravel.com",
            "role": "Developer"
        }
    ],
    "require": {
        "php" : ">=7.1.3",
        "illuminate/support": "5.6.*|5.7.*",
        "illuminate/auth": "5.6.*|5.7.*",
        "illuminate/mail": "5.6.*|5.7.*",
        "anhskohbo/no-captcha": "^3.0.1",
        "doctrine/dbal": "~2.5",
        "laravolt/ui":"~0.5",
        "laravolt/password":"^1.1"
    },
    "require-dev": {
        "phpunit/phpunit": "^7.0",
        "mockery/mockery": "^1.0",
        "orchestra/testbench-browser-kit": "3.6.*|3.7.*",
        "orchestra/database": "^3.4"
    },
    "suggest": {
        "subfission/cas": "Required if cas login enabled",
        "adldap2/adldap2-laravel": "Required if ldap login enabled"
    },
    "autoload": {
        "psr-4": {
            "Laravolt\\Auth\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Laravolt\\Auth\\Tests\\": "tests"
        }
    },
    "scripts": {
        "test": "vendor/bin/phpunit"
    },
    "extra": {
        "branch-alias": {
            "dev-master": "5.0-dev"
        },
        "laravel": {
            "providers": [
                "Laravolt\\Auth\\ServiceProvider",
                "Laravolt\\Password\\ServiceProvider",
                "Laravolt\\Ui\\ServiceProvider"
            ]
        }
    }
}

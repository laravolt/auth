{
    "name": "laravolt/auth",
    "description": "Laravel auth extended",
    "keywords": [
        "laravolt",
        "auth",
        "laravel"
    ],
    "homepage": "https://github.com/laravolt/auth",
    "license": "MIT",
    "authors": [
        {
            "name": "Bayu Hendra Winata",
            "email": "id.laravel@gmail.com",
            "homepage": "http://id-laravel.com",
            "role": "Developer"
        }
    ],
    "require": {
        "php" : ">=7.0",
        "illuminate/support": "5.4.*|5.5.*|5.6.*|5.7.*|5.8.*",
        "illuminate/auth": "5.4.*|5.5.*|5.6.*|5.7.*|5.8.*",
        "illuminate/mail": "5.4.*|5.5.*|5.6.*|5.7.*|5.8.*",
        "anhskohbo/no-captcha": "^3.0.1",
        "doctrine/dbal": "~2.5",
        "laravolt/ui":"^0.2.0",
        "laravolt/password":"^1.1"
    },
    "require-dev": {
        "phpunit/phpunit": "~5.7",
        "mockery/mockery": "~0.9.4",
        "orchestra/testbench-browser-kit": "^3.4",
        "orchestra/database": "^3.4"
    },
    "autoload": {
        "psr-4": {
            "Laravolt\\Auth\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Laravolt\\Auth\\Tests\\": "tests"
        }
    },
    "scripts": {
        "test": "vendor/bin/phpunit"
    },
    "extra": {
        "branch-alias": {
            "dev-master": "5.0-dev"
        },
        "laravel": {
            "providers": [
                "Laravolt\\Auth\\ServiceProvider",
                "Laravolt\\Password\\ServiceProvider",
                "Laravolt\\Ui\\ServiceProvider"
            ]
        }
    }
}
